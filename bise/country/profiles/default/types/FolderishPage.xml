<?xml version="1.0"?>
<object name="FolderishPage" meta_type="Dexterity FTI"
   i18n:domain="cs.folderishpage"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">FolderishPage</property>
 <property name="description" i18n:translate="">A folderish page</property>
 <property name="icon_expr">string:${portal_url}/folder_icon.png</property>
 <property name="factory">cs.folderishpage.folderishpage</property>
 <property
    name="add_view_expr">string:${folder_url}/++add++FolderishPage</property>
 <property name="link_target"></property>
 <property name="immediate_view">view</property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">False</property>
 <property name="allowed_content_types"/>
 <property name="allow_discussion">False</property>
 <property name="default_view">view</property>
 <property name="view_methods">
  <element value="view"/>
  <element value="contentsview"/>
  <element value="folder_listing"/>
  <element value="atlasview"/>
  <element value="maesgalleryview"/>
 </property>
 <property name="default_view_fallback">False</property>
 <property name="add_permission">cs.folderishpage.AddFolderishPage</property>
 <property
    name="klass">cs.folderishpage.folderishpage.FolderishPage</property>
 <property name="behaviors">
  <element value="plone.app.dexterity.behaviors.metadata.IDublinCore"/>
  <element value="plone.app.content.interfaces.INameFromTitle"/>
  <element
     value="plone.app.dexterity.behaviors.exclfromnav.IExcludeFromNavigation"/>
  <element value="plone.app.relationfield.behavior.IRelatedItems"/>
  <element
     value="plone.app.dexterity.behaviors.nextprevious.INextPreviousToggle"/>
  <element value="plone.app.dexterity.behaviors.discussion.IAllowDiscussion"/>
  <element
     value="Products.CMFPlone.interfaces.constrains.ISelectableConstrainTypes"/>
  <element value="bise.theme.behaviors.sources.IExtraFolderishPage"/>
  <element value="bise.theme.behaviors.tagging.ICatalogueTags"/>
  <!-- <element value="plone.app.stagingbehavior.interfaces.IStagingSupport"/> -->
  <element value="plone.app.lockingbehavior.behaviors.ILocking"/>
 </property>
 <property
    name="schema">cs.folderishpage.folderishpage.IFolderishPage</property>
 <property
    name="model_source">&lt;model xmlns="http://namespaces.plone.org/supermodel/schema"&gt;
      &lt;schema /&gt;
  &lt;/model&gt;</property>
 <property name="model_file"></property>
 <property name="schema_policy">dexterity</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="@@edit"/>
 <alias from="sharing" to="@@sharing"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    description="" icon_expr="" link_target=""
    url_expr="string:${object_url}/" visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    description="" icon_expr="" link_target=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="Modify portal content"/>
 </action>
</object>
