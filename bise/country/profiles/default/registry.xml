<registry>

  <record
    name="plone.app.iterate.interfaces.IIterateSettings.checkout_workflow_policy"
    interface="plone.app.iterate.interfaces.IIterateSettings"
    field="checkout_workflow_policy">
    <field type="plone.registry.field.ASCIILine">
      <default>checkout_workflow_policy</default>
      <title>Checkout workflow policy</title>
    </field>
    <value>checkout_workflow_policy</value>
  </record>

  <record
    name="plone.app.iterate.interfaces.IIterateSettings.enable_checkout_workflow"
    interface="plone.app.iterate.interfaces.IIterateSettings"
    field="enable_checkout_workflow">
    <field type="plone.registry.field.Bool">
      <default>False</default>
      <required>False</required>
      <title>Enable checkout workflow</title>
    </field>
    <value>True</value>
  </record>

  <record name="plone.app.tiles">
    <value purge="false">
      <element>bise.countryheader</element>
    </value>
  </record>

  <record name="collective.cover.controlpanel.ICoverSettings.available_tiles">
    <value purge="false">
      <element>bise.countryheader</element>
    </value>
  </record>

</registry>
